{
  "name": "hobnob-slv",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "build": "rimraf dist && babel src -d dist",
    "serve": "yarn run build && dotenv node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:e2e": "dotenv cucumber-js -- spec/cucumber/features --require-module @babel/register --require spec/cucumber/steps",
    "watch": "nodemon -w src --exec yarn run serve",
    "fix": "eslint src --fix",
    "lint": "eslint src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint"
    }
  },
  "repository": "https://github.com/doniisetiiawan/hobnob-slv.git",
  "author": "Doni Setiawan <donisetiawan.050@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.6.0",
    "cucumber": "^5.1.0",
    "dotenv-cli": "^2.0.1",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.5",
    "ndb": "^1.1.5",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "superagent": "^5.1.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0"
  }
}
